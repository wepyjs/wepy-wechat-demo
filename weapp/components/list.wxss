.list{margin-top:40rpx;background-color:#fff;border-bottom:1px solid #eee}.list .item{height:100rpx;position:relative;box-sizing:border-box;padding:20rpx;border-bottom:1px solid #eee}.list .item .icon{width:60rpx;height:60rpx;position:absolute;margin-left:15rpx}.list .item .arrow_left{position:absolute;right:20rpx;width:30rpx;height:40rpx;margin-top:-32rpx}.list .item .title{padding:18rpx 0 0 100rpx;display:block;font-size:33rpx}

